	Journal = {R\&D Management},
	Number = {4},
	Pages = {342-356},
	Title = {Community engineering for innovations: the ideas competition as a method to nurture a virtual community for innovations},
	Url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1467-9310.2009.00564.x},
	Volume = {39},
	Year = {2009},
	Bdsk-Url-1 = {https://onlinelibrary.wiley.com/doi/abs/10.1111/j.1467-9310.2009.00564.x},
