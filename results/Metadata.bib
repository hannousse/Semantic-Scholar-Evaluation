	Editor = {Seruca, Isabel and Cordeiro, Jos{\'e} and Hammoudi, Slimane and Filipe, Joaquim},
	Isbn = {978-1-4020-3675-0},
	Pages = {203-210},
	Publisher = {Springer Netherlands},
	Title = {Towards a Meta Model for Describing Communication - How to address interoperability on a pragmatic level},
	Year = {2004},
	Bdsk-Url-1 = {https://doi.org/10.1007/1-4020-3675-2_24}}

