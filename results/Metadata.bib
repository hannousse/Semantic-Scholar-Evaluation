	Issn = {2352-2208},
	Journal = {Journal of Logical and Algebraic Methods in Programming},
	Keywords = {Extended feature model with cardinality, Specialization and generalization of SPLS, Multiple multi-level promotions in Alloy},
	Number = {5},
