	Issn = {1947-8194},
	Journal = {International Journal of Information System Modeling and Design},
	Keywords = {Intentional Process Models; Machine Learning; Process Mining; Hidden Markov Models},
	Number = {2},
	Pages = {43--71},
	Publisher = {IGI Global},
	Title = {Mining Users' Intents from Logs},
	Url = {http://dx.doi.org/10.4018/IJISMD.2015040102},
	Volume = {6},
	Year = {2015},
	Bdsk-Url-1 = {http://dx.doi.org/10.4018/IJISMD.2015040102},
