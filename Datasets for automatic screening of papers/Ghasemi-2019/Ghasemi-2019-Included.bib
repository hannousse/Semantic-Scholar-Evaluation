	Keywords = {intention mining; process mining; flexible processes; process aware information systems; process recommendations},
	Pages = {473-487},
	Publisher = {Springer International Publishing},
	Title = {What Shall I Do Next? Intention Mining for Flexible Process Enactment},
	Year = {2014},
	Bdsk-Url-1 = {https://doi.org/10.1007/978-3-319-07881-6_32}}

@phdthesis{Khodabandelou:2014a,
	Abstract = {So far, process mining techniques suggested to model processes in terms of tasks that occur during the enactment of a process. However, research on process modeling has illustrated that many issues, such as lack of flexibility or adaptation, are solved more effectively when intentions are explicitly specified. This thesis presents a novel approach of process mining, called Map Miner Method (MMM). This method is designed to automate the construction of intentional process models from traces. MMM uses Hidden Markov Models to model the relationship between users' activities and the strategies (i.e., the different ways to fulfill the intentions). The method also includes two specific algorithms developed to infer users' intentions and construct intentional process model (Map), respectively. MMM can construct Map process models with different levels of granularity (pseudo-Map and Map process models) with respect to the Map metamodel formalism. The entire proposed method was applied and validated on practical traces in a large-scale experiment, on event logs of developers of Eclipse UDC (Usage Data Collector). The resulting Map process models provide a precious understanding of the processes followed by the developers, and also provide feedback on the effectiveness and demonstrate scalability of MMM in terms of traces. Map Miner tool has been developed to enable practicing the proposed approach. This permits users to obtain the pseudo-Map and Map process model out of traces.},
	Author = {Ghazaleh Khodabandelou},
	Date-Added = {2019-05-12 19:30:04 +0100},
	Date-Modified = {2020-10-24 19:08:37 +0100},
	Keywords = {Intentional Process Models; Machine Learning; process mining; Hidden Markov Models},
	Pages = {194},
	School = {Universit{\'e} Panth{\'e}on-Sorbonne - Paris I},
	Title = {Mining Intentional Process Models},
	Type = {Theses},
	Year = {2014}}

@inproceedings{Khodabandelou:2014,
	Abstract = {Learning humans' behavior from activity logs requires choosing an adequate machine learning technique regarding the situation at hand. This choice impacts significantly results reliability. In this paper, Hidden Markov Models (HMMs) are used to build intentional process models (Maps) from activity logs. Since HMMs parameters require to be learned, the main contribution of this paper is to compare supervised and unsupervised learning approaches of HMMs. After a theoretical comparison of both approaches, they are applied on two controlled experiments to compare the Maps thereby obtained. The results demonstrate using supervised learning leads to a poor performance because it imposes binding conditions in terms of data labeling, introduces inherent humans' biases, provides unreliable results in the absence of ground truth, etc. Instead, unsupervised learning obtains efficient Maps with a higher performance and lower humans' effort.},
	Address = {Berlin, Heidelberg},
	Author = {Khodabandelou, Ghazaleh and Hug, Charlotte and Deneck{\`e}re, Rebecca and Salinesi, Camille},
	Booktitle = {Enterprise, Business-Process and Information Systems Modeling},
