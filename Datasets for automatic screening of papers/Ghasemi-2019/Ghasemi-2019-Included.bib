	Keywords = {Key performance indicators (KPI); Electronic Data Interchange (EDI); EDIFACT},
	Pages = {276-283},
	Publisher = {IEEE},
	Title = {Identifying Inter-organizational Key Performance Indicators from EDIFACT Messages},
	Url = {http://dx.doi.org/10.1109/CBI.2013.46},
