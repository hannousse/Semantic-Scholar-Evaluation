	Isbn = {978-3-662-43745-2},
	Keywords = {Supervised Learning; Unsupervised Learning; Intentional Process Modeling; Hidden Markov Models},
	Pages = {215-229},
	Publisher = {Springer Berlin Heidelberg},
	Title = {Supervised vs. Unsupervised Learning for Intentional Process Model Discovery},
	Year = {2014},
