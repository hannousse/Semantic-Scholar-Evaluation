	Isbn = {9781450328630},
	Journal = {Proceedings of the 11th Working Conference on Mining Software Repositories - MSR 2014},
	Keywords = {Event logs Mining; Software Development Process; Intentional Process Modeling},
	Pages = {282--291},
	Publisher = {ACM Press},
	Title = {Unsupervised discovery of intentional process models from event logs},
	Url = {http://dx.doi.org/10.1145/2597073.2597101},
	Year = {2014},
