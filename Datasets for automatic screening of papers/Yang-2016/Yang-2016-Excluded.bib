	Volume = {46},
	Year = {2012},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S1877042812022185},
	Bdsk-Url-2 = {https://doi.org/10.1016/j.sbspro.2012.06.482}}

