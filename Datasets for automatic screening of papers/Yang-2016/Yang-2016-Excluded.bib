	Journal = {ACM Trans. Archit. Code Optim.},
	Keywords = {WCET, code positioning, embedded systems},
	Number = {4},
	Numpages = {31},
	Pages = {335--365},
	Publisher = {ACM},
	Title = {Improving WCET by Applying a WC Code-positioning Optimization},
