	Issue_Date = {June 2005},
	Journal = {SIGMETRICS Perform. Eval. Rev.},
	Keywords = {distributed hash table, overlay network, peer-to-peer system},
	Number = {1},
	Numpages = {12},
	Pages = {14--25},
	Publisher = {ACM},
	Title = {Low Traffic Overlay Networks with Large Routing Tables},
