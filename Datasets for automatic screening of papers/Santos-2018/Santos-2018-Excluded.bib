	Journal = {Vaccine},
	Keywords = {BLS, Rotavirus, Subunit vaccine},
	Number = {1},
	Pages = {136 - 145},
	Title = {Brucella spp. lumazine synthase as a bovine rotavirus antigen delivery system},
