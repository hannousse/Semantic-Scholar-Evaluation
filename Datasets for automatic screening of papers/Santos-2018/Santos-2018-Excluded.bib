	Keywords = {Mandarin peel oil, Countercurrent extraction, Desorption, Terpenes, Supercritical fractionation},
	Note = {8th International Symposium on Supercritical Fluids, 5-8 November, 2006, Kyoto, Japan},
	Number = {3},
	Pages = {315 - 324},
	Title = {Deterpenation of mandarin (Citrus reticulata) peel oils by means of countercurrent multistage extraction and adsorption/desorption with supercritical CO2},
