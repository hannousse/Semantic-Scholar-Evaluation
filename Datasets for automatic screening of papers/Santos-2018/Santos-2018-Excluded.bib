	Journal = {Brain Research Reviews},
	Keywords = {Olfaction, Smell, Orbitofrontal cortex, Comparative neuroanatomy, Functional neuroimaging, Meta-analysis},
	Number = {2},
	Pages = {287 - 304},
	Title = {On the scent of human olfactory orbitofrontal cortex: Meta-analysis and comparison to non-human primates},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165017305001189},
	Volume = {50},
	Year = {2005},
