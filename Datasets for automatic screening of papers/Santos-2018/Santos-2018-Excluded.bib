	Issn = {0091-3057},
	Journal = {Pharmacology Biochemistry and Behavior},
	Keywords = {Atipamezole, Dexmedetomidine, Yohimbine, α-Adrenoreceptor, Sexual motivation, Ambulatory activity},
	Number = {3},
	Pages = {360 - 369},
	Title = {The adrenergic α2 receptor and sexual incentive motivation in male rats},
