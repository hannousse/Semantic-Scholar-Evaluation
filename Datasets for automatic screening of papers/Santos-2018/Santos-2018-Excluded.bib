	Date-Modified = {2019-08-31 17:50:56 +0100},
	Doi = {https://doi.org/10.1016/j.applanim.2017.05.013},
	Issn = {0168-1591},
	Journal = {Applied Animal Behaviour Science},
	Keywords = {Sensory stimulation, Toothed whales, Taste, Enrichment, Spontaneous behaviour, Chemoreception},
	Pages = {120 - 126},
	Title = {Do bottlenose dolphins display behavioural response to fish taste?},
	Url = {http://www.sciencedirect.com/science/article/pii/S0168159117301557},
	Volume = {194},
	Year = {2017},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0168159117301557},
