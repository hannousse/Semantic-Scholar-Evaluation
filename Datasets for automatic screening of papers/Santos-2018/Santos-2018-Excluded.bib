	Doi = {https://doi.org/10.1016/j.applanim.2016.01.014},
	Issn = {0168-1591},
	Journal = {Applied Animal Behaviour Science},
	Keywords = {Brown Norway rat, Horse, Allelochemics, Olfaction, Penile erection, Ketones},
	Pages = {128 - 135},
