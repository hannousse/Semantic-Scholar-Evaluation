	Keywords = {Abdominal pain, Analgesics, Acute abdomen, Emergency department, Opioids},
	Number = {4},
	Pages = {291 - 296},
	Title = {Critically reappraising the literature-driven practice of analgesia administration for acute abdominal pain in the emergency room prior to surgical evaluation},
