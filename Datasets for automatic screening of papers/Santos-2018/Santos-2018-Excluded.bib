	Keywords = {Longitudinal studies, Neoplasms, Olfaction disorders, Signs and symptoms, Taste disorders},
	Pages = {232 - 241},
	Title = {A longitudinal study of changing characteristics of self-reported taste and smell alterations in patients treated for lung cancer},
