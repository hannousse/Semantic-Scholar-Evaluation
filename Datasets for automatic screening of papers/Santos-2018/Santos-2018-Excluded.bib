	Issn = {0028-3932},
	Journal = {Neuropsychologia},
	Keywords = {Cross-modal, Cross-sensory, Texture, Taste, Food, Synaesthesia, Hedonic},
	Note = {Special Issue: Synaesthesia and Multisensory Processes},
	Pages = {58 - 64},
	Title = {Cross-modal tactile--taste interactions in food evaluations},
	Url = {http://www.sciencedirect.com/science/article/pii/S0028393215300968},
	Volume = {88},
	Year = {2016},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0028393215300968},
