	Keywords = {Chemical intolerance, Olfactory, Trigeminal, Sensitization, Smell, fMRI},
	Number = {2, Part B},
	Pages = {503 - 509},
	Title = {Short-term olfactory sensitization involves brain networks relevant for pain, and indicates chemical intolerance},
	Url = {http://www.sciencedirect.com/science/article/pii/S1438463916304400},
	Volume = {220},
