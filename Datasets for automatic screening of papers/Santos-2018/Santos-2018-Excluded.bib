	Keywords = {Dendrobatidae, olfaction, parental care, phytotelmata, predation, , reproductive strategy},
	Number = {6},
	Pages = {1147 - 1154},
	Title = {The smell of success: choice of larval rearing sites by means of chemical cues in a Peruvian poison frog},
	Url = {http://www.sciencedirect.com/science/article/pii/S0003347211000728},
	Volume = {81},
