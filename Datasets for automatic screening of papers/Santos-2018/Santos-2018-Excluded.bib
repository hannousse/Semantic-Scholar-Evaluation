	Journal = {Meat Science},
	Keywords = {Boar taint, Consumer acceptance, Androstenone, Skatole},
	Number = {2},
	Pages = {330 - 337},
	Title = {Acceptability of entire male pork with various levels of androstenone and skatole by consumers according to their sensitivity to androstenone},
	Url = {http://www.sciencedirect.com/science/article/pii/S0309174011002695},
	Volume = {90},
	Year = {2012},
