	Issn = {0031-9384},
	Journal = {Physiology & Behavior},
	Keywords = {Behaviour, Essential amino acids, Nutritional wisdom, , Self-selection},
	Number = {3},
	Pages = {639 - 644},
	Title = {Dietary self-selection of protein-unbalanced diets supplemented with three essential amino acids in Nile tilapia},
