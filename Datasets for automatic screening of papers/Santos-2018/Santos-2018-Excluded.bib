	Journal = {Neuropsychologia},
	Keywords = {Chemosensation, Chemosignal, Olfaction, Anger, Threat},
	Pages = {187 - 198},
	Title = {Chemosensory danger detection in the human brain: Body odor communicating aggression modulates limbic system activation},
	Url = {http://www.sciencedirect.com/science/article/pii/S0028393217300647},
	Volume = {99},
	Year = {2017},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0028393217300647},
