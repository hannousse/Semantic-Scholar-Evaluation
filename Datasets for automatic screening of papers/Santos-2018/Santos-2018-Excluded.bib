	Number = {6},
	Pages = {1036 - 1042},
	Title = {Monitoring aroma changes during human milk storage at +4 $\,^{\circ}$C by sensory and quantification experiments},
	Url = {http://www.sciencedirect.com/science/article/pii/S0261561413000411},
	Volume = {32},
