	Issn = {0950-3293},
	Journal = {Food Quality and Preference},
	Keywords = {Emotion, Hedonic response, Feelings, Color, Expectation, Perfume},
	Note = {Ninth Pangborn Sensory Science Symposium},
	Number = {2},
	Pages = {161 - 169},
	Title = {Seeing, smelling, feeling! Is there an influence of color on subjective affective responses to perfumed fabric softeners?},
	Url = {http://www.sciencedirect.com/science/article/pii/S0950329312001255},
	Volume = {27},
	Year = {2013},
