	Journal = {Waste Management},
	Keywords = {Self-heating, Self-combustion, Thermal runaway, Wetting heat, Tannery sludge},
	Number = {1},
	Pages = {129 - 137},
	Title = {Self-heating of dried wastewater sludge},
	Url = {http://www.sciencedirect.com/science/article/pii/S0956053X12003807},
