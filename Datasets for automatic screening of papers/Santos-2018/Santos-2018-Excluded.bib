	Keywords = {control engineering computing;control system synthesis;flaw detection;formal logic;formal verification;graph grammars;logic controller verification;algorithmic verification;sequential function chart controller;a-posteriori analysis;design flaw detection;synthesis algorithm;description language;industrial controllers;cyclic operation hardware mode;timers;real time behavior;time automaton modeling;graph grammar;evaporation system control;Automatic control;Algorithm design and analysis;Logic design;Industrial control;Automata;Control systems;Manufacturing automation;Nonlinear dynamical systems;Process control;Hardware},
	Month = {Sep.},
