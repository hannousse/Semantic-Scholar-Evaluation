	Issn = {0950-3293},
	Journal = {Food Quality and Preference},
	Keywords = {Odor, Olfaction, Communication, Description, Language, Recognition},
	Number = {5},
	Pages = {443 - 452},
	Title = {Sharing an olfactory experience: The impact of oral communication},
	Url = {http://www.sciencedirect.com/science/article/pii/S0950329309001797},
	Volume = {21},
	Year = {2010},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0950329309001797},
