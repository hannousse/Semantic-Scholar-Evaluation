	Date-Added = {2019-08-31 17:50:56 +0100},
	Date-Modified = {2019-08-31 17:50:56 +0100},
	Doi = {https://doi.org/10.1016/j.bbagen.2016.12.008},
	Issn = {0304-4165},
	Journal = {Biochimica et Biophysica Acta (BBA) - General Subjects},
	Keywords = {G-quadruplex, Folding landscape, Molecular dynamics, Simulation force fields},
	Note = {G-quadruplex},
	Number = {5, Part B},
	Pages = {1246 - 1263},
	Title = {Folding of guanine quadruplex molecules--funnel-like mechanism or kinetic partitioning? An overview from MD simulation studies},
