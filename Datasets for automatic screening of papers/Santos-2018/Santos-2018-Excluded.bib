@article{VANBOXSTAEL201485,
	Abstract = {Based on an on-line survey using questionnaires, information on Belgian consumers (n = 907) regarding their understanding and attitude towards shelf life labels and dates was collected. 80.1% of the respondents were familiar with the terminology of the label use by and best before while 69.6% indicated to know the difference between the meaning of the two labels. Judging edibility of food products at home occurs mainly by a combination of checking visually and smelling (82.5%), followed by looking at the shelf life date (67.5%) or tasting (50.4%). About half of the consumers is aware of and appreciates the difference between the two labels during this judgment. Most of the consumers interpret shelf life labels and dates with some flexibility, with variation depending upon the type of food product under consideration. Overall, the share of consumers willing to eat expired refrigerated products was lower than for expired products stored at ambient temperature: 19.1% for refrigerated raw products (e.g. meat and fish); 34.7% for refrigerated ready-to-eat products (e.g. deli-meat, cheese products, smoked fish); 61.0% for canned products (e.g. conserves) and drinks (e.g. beer) and lastly 69.3% for other products generally stored at ambient temperature (e.g. bread, unopened UHT milk, chocolate, cereals). Differences linked to age and gender are discussed. The suboptimal understanding and application of the EU date label framework by consumers may lead on the one hand to increased food disease burden by consuming certain expired use by products allowing growth of pathogens. On the other hand discarding food at the shelf life data in case of a best before date contributes to food waste by consumers. A better understanding of the shelf life labels use by (referring to food safety) and best before dates (referring to food quality) by consumers should be promoted.},
	Author = {S. Van Boxstael and F. Devlieghere and D. Berkvens and A. Vermeulen and M. Uyttendaele},
	Date-Added = {2019-08-31 17:50:56 +0100},
	Date-Modified = {2019-08-31 17:50:56 +0100},
	Doi = {https://doi.org/10.1016/j.foodcont.2013.08.043},
	Issn = {0956-7135},
	Journal = {Food Control},
	Keywords = {Consumer survey, Shelf life labels, , , Food safety, Food waste},
	Pages = {85 - 92},
	Title = {Understanding and attitude regarding the shelf life labels and dates on pre-packed food products by Belgian consumers},
	Url = {http://www.sciencedirect.com/science/article/pii/S0956713513004350},
	Volume = {37},
	Year = {2014},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0956713513004350},
