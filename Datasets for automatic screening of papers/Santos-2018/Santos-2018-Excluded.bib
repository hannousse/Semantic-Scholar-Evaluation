	Journal = {Veterinary Parasitology},
	Keywords = {, Bovine lungworm, Antibody titre, Antibody level, ELISA, Milk ELISA, BTM ELISA, Infection dose, Reinfection},
	Pages = {47 - 53},
	Title = {Serological lessons from the bovine lungworm Dictyocaulus viviparus: Antibody titre development is independent of the infection dose and reinfection shortens seropositivity},
	Url = {http://www.sciencedirect.com/science/article/pii/S0304401717302248},
