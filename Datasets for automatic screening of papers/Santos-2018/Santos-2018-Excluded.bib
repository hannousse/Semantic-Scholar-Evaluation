	Journal = {Animal Behaviour},
	Keywords = {anal droplet, antennal removal, chemosensory cue, courtship song, , , mate choice, phenotypic manipulation, visual cue, wing display},
	Number = {1},
	Pages = {217 - 227},
	Title = {Different sensory modalities are required for successful courtship in two species of the Drosophila willistoni group},
	Url = {http://www.sciencedirect.com/science/article/pii/S0003347211004787},
	Volume = {83},
	Year = {2012},
