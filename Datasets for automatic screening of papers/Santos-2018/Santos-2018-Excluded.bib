	Issn = {2213-2198},
	Journal = {The Journal of Allergy and Clinical Immunology: In Practice},
	Keywords = {Aspirin-exacerbated respiratory disease, Aspirin-induced asthma, Desensitization, Survey, Quality of life},
	Number = {5},
	Pages = {711 - 718},
	Title = {Survey-Defined Patient Experiences With Aspirin-Exacerbated Respiratory Disease},
