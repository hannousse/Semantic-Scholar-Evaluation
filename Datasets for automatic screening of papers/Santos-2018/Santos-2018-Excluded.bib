	Keywords = {Android (operating system);mobile computing;program debugging;public domain software;software maintenance;source code (software);code smells;Android applications;maintainability problems;mask bugs;software sector;mobile applications;desktop applications;mobile development;open-source corpus;Androids;Humanoid robots;Mobile applications;Mobile communication;Smart phones;Java;Software engineering;Code Smell;Android;Java;literature review},
	Month = {May},
