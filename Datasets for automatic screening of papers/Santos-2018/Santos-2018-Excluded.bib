	Journal = {Research in Developmental Disabilities},
	Keywords = {Down syndrome, Intellectual disability, Aging, Dementia, ADL},
	Pages = {532 - 536},
	Title = {Are early onset aging conditions correlated to daily activity functions in youth and adults with Down syndrome?},
