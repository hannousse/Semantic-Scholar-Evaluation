	Issn = {0950-3293},
	Journal = {Food Quality and Preference},
	Keywords = {Emotion, Hedonic response, Feelings, Expectation, Quality, Food odor},
	Note = {4th European Conference on Sensory and Consumer Research - A Sense of Quality},
	Number = {2},
	Pages = {125 - 133},
	Title = {Influence of food odorant names on the verbal measurement of emotions},
	Url = {http://www.sciencedirect.com/science/article/pii/S0950329311001285},
