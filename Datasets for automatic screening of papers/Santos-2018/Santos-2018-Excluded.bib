	Issn = {0098-5589},
	Journal = {IEEE Transactions on Software Engineering},
	Keywords = {software engineering;spreadsheet programs;spreadsheets;CACheck;numerical computation;ad hoc modifications;EUSES corpora;Enron corpora;smelly cell arrays;Semantics;Maintenance engineering;Software;Computer science;Nonhomogeneous media;Electronic mail;Business;Spreadsheet;cell array;ambiguous computation smell},
	Month = {March},
	Number = {3},
