	Number = {2},
	Pages = {832 - 839},
	Title = {Combined electronic nose and tongue for a flavour sensing system},
	Url = {http://www.sciencedirect.com/science/article/pii/S0925400511001705},
	Volume = {156},
	Year = {2011},
