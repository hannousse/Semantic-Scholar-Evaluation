	Doi = {10.1109/CISIS.2010.120},
	Keywords = {handicapped aids;haptic interfaces;disabled people;information society;tactile information;multisensory communication;haptization based communication framework;people with disabilities;Humans;Microcomputers;Personal communication networks;Mobile handsets;Mobile communication;Auditory system;Skin;Pain;Temperature sensors;Web pages;disabled people;haptization;digital divide;the weak in information technology;information accesibility},
