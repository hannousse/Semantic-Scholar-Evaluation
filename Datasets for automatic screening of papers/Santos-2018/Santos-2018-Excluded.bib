	Issn = {0957-4174},
	Journal = {Expert Systems with Applications},
	Keywords = {Autonomous agents, Software refactoring, Smells detection, Smells correction, Code quality},
	Number = {3},
	Pages = {1652 - 1664},
	Title = {AutoRefactoring: A platform to build refactoring agents},
	Url = {http://www.sciencedirect.com/science/article/pii/S0957417414005612},
	Volume = {42},
	Year = {2015},
