	Issue_Date = {January 2015},
	Journal = {Personal Ubiquitous Comput.},
	Keywords = {Activity sensing, Electronic nose, Gas sensors},
	Number = {1},
	Numpages = {14},
	Pages = {189--202},
	Publisher = {Springer-Verlag},
	Title = {uSmell: Exploring the Potential for Gas Sensors to Classify Odors in Ubicomp Applications Relative to Airflow and Distance},
