	Keywords = {Atmospheric argon micro-plasma system, Dimethyl sulfide, Decomposition, Treatment efficiency, Recycling},
	Pages = {185 - 192},
	Title = {Conversion of emitted dimethyl sulfide into eco-friendly species using low-temperature atmospheric argon micro-plasma system},
