	Issn = {0022-0981},
	Journal = {Journal of Experimental Marine Biology and Ecology},
	Keywords = {Great Barrier Reef, Nutrient enrichment, Scleractinia, Sedimentation, Sediment properties, Terrestrial runoff},
	Number = {1},
	Pages = {18 - 32},
	Title = {Sedimentation stress in a scleractinian coral exposed to terrestrial and marine sediments with contrasting physical, organic and geochemical properties},
