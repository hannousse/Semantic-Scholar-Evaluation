	Issn = {1539-9087},
	Issue_Date = {April 2017},
	Journal = {ACM Trans. Embed. Comput. Syst.},
	Keywords = {Data recovery, file carving, mobile security, secure deletion},
	Number = {2},
	Numpages = {22},
	Pages = {61:1--61:22},
