	Journal = {The Journal of Sexual Medicine},
	Keywords = {Inflatable Penile Prosthesis, Antibiotic Selection, Infection, Erectile Dysfunction, Survey of Urologic Practice in Preventing Penile Implant Infections},
	Number = {5},
	Pages = {1521 - 1528},
	Title = {Antibiotic Patterns with Inflatable Penile Prosthesis Insertion},
