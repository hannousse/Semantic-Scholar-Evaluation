	Journal = {Journal of Reproduction and Contraception},
	Keywords = {intrauterine devices, vaginitis, contraception, Ibadan, Nigeria},
	Number = {4},
	Pages = {247 - 255},
	Title = {Study on Vaginitis Among Intrauterine Contraceptive Device Users in Ibadan, South-western Nigeria},
