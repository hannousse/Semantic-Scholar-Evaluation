	Journal = {Behavioural Brain Research},
	Keywords = {Anxiety, Animal model, TMT, Cat odor, Corticosterone, HAB},
	Number = {1},
	Pages = {230 - 234},
	Title = {Genetic predisposition to anxiety-related behavior predicts predator odor response},
