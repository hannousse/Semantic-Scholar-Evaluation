	Keywords = {dcoupling, dependency oriented complexity metric for an artifact affected by ripples (DOCMA(AR)), dependency oriented complexity metric for an artifact causing ripples (DOCMA(CR)), dependency oriented complexity metric for structure (DOCMS(R))},
	Number = {4},
	Numpages = {7},
	Pages = {1--7},
