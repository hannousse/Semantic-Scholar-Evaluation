	Keywords = {Web Services, Web Service publication, Web Service discovery, Web Service discoverability anti-patterns},
	Note = {Special Section on the Programming Languages Track at the 23rd ACM Symposium on Applied Computing},
	Number = {11},
	Pages = {1001 - 1021},
	Title = {Improving Web Service descriptions for effective service discovery},
	Url = {http://www.sciencedirect.com/science/article/pii/S0167642310000134},
	Volume = {75},
	Year = {2010},
