	Keywords = {microkernel, performance, security},
	Location = {Kennesaw, Georgia},
	Numpages = {9},
	Pages = {25--33},
	Publisher = {ACM},
	Series = {InfoSecCD '11},
	Title = {Measures to Improve Security in a Microkernel Operating System},
