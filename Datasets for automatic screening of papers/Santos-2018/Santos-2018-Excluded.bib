	Issn = {1352-2310},
	Journal = {Atmospheric Environment},
	Keywords = {Odour dispersion, Concentration fluctuations, Physical modelling, Averaging time, Intermittency, Persistence, 90-percentile},
	Number = {1},
	Pages = {81 - 93},
	Title = {Unsteady characteristics of the dispersion process in the vicinity of a pig barn. Wind tunnel experiments and comparison with field data},
	Url = {http://www.sciencedirect.com/science/article/pii/S135223100300801X},
