	Keywords = {Supercritical fluid extraction, Omega-3 fatty acids, Fish oil, Hake by-product, Carbon dioxide},
	Number = {2},
	Pages = {215 - 226},
	Title = {Supercritical fluid extraction of the omega-3 rich oil contained in hake (Merluccius capensis--Merluccius paradoxus) by-products: Study of the influence of process parameters on the extraction yield and oil quality},
