	Journal = {Expert Systems with Applications},
	Keywords = {Differential electronic nose, Support vector machine, Cigarette recognition},
	Number = {10},
	Pages = {9886 - 9891},
	Title = {Differential electronic nose and support vector machine for fast recognition of tobacco},
	Url = {http://www.sciencedirect.com/science/article/pii/S0957417412004265},
	Volume = {39},
	Year = {2012},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0957417412004265},
