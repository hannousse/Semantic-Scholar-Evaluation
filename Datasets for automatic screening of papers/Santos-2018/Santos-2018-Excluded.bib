	Keywords = {object-oriented programming;software maintenance;OBEY methodology;optimal batched refactoring plan execution;class responsibility redistribution;software reengineering practice;object-oriented software evolution;OO software evolution;refactoring engine;coupling-increasing middle man bad smell;open-source software project;Couplings;Engines;Software systems;Measurement;Optimization;Context;Reengineering;class responsibility redistribution;batched refactoring execution;change impact analysis;optimization},
	Month = {Sep.},
	Number = {9},
