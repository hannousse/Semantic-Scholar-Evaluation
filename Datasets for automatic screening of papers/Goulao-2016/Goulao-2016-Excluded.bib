	Journal = {Gender Medicine},
	Keywords = {posttraumatic stress disorder, gender, World Trade Center, primary care},
	Number = {2},
	Pages = {76 - 87},
	Title = {Gender differences in posttraumatic stress disorderamong primary care patients after the World Trade Center attack of September 11, 2001},
