	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Systematic review, Meta-analysis, Depressive disorder, Antidepressants},
	Pages = {48 - 58},
	Title = {Efficacy and tolerability of antidepressants for sub-threshold depression and for mild major depressive disorder},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165032714002729},
	Volume = {166},
	Year = {2014},
