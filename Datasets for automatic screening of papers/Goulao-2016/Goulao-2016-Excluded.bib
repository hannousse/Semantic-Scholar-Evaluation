	Journal = {Drug and Alcohol Dependence},
	Keywords = {Comorbidity, Alcohol use disorders, Major depressive episode, Solitary drinking, Affect regulation},
	Number = {2},
	Pages = {145 - 151},
	Title = {Alcohol involvement as a function of co-occurring alcohol use disorders and major depressive episode: Evidence from the National Epidemiologic Survey on Alcohol and Related Conditions},
