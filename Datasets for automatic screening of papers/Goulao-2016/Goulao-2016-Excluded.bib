	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Depression, Mental disorder, Prevalence, Structured diagnostic interview, Lifetime},
	Number = {1},
	Pages = {39 - 44},
	Title = {Quantifying point prevalence of major depressive episode using lifetime structured diagnostic interviews},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165032709002195},
	Volume = {121},
	Year = {2010},
