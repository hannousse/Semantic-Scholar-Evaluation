	Issn = {0277-9536},
	Journal = {Social Science & Medicine},
	Keywords = {Income inequality, Human development index, Major depressive episode, Mental health, Multilevel modeling, World Health Organization (WHO)},
	Number = {4},
	Pages = {529 - 539},
	Title = {The association of major depressive episodes with income inequality and the human development index},
	Url = {http://www.sciencedirect.com/science/article/pii/S0277953608002098},
