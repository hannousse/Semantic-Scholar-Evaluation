	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Auditory verbal hallucinations, Mood disorders, Bipolar disorder, Major depressive disorder},
	Pages = {18 - 28},
	Title = {Auditory verbal hallucinations in bipolar disorder (BD) and major depressive disorder (MDD): A systematic review},
	Url = {http://www.sciencedirect.com/science/article/pii/S016503271500347X},
	Volume = {184},
