	Journal = {European Neuropsychopharmacology},
	Keywords = {Inflammation, Cytokines, Inflammatory mediators, Suicidal behavior},
	Number = {12},
	Pages = {1672 - 1686},
	Title = {The role of inflammatory cytokines in suicidal behavior: A systematic review},
	Url = {http://www.sciencedirect.com/science/article/pii/S0924977X13001776},
