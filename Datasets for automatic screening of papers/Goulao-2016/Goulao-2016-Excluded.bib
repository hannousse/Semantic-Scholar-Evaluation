	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Depressive disorder, Dysthymic disorder, Double depression, Course, Epidemiology},
	Number = {3},
	Pages = {450 - 459},
	Title = {The three-year naturalistic course of major depressive disorder, dysthymic disorder and double depression},
