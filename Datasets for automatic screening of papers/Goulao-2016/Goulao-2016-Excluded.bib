	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Depression, Functional limitations, General population},
	Number = {3},
	Pages = {363 - 371},
	Title = {Functioning after a major depressive episode: complete or incomplete recovery?},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165032704000904},
