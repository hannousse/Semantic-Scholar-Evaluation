	Journal = {International Journal of Applied Earth Observation and Geoinformation},
	Keywords = {remote sensing, GIS, GPS, soil, landform, landscape forming processes, land cover, land use, land evaluation},
	Number = {1},
	Pages = {3 - 10},
	Title = {Advances in the application of remote sensing and GIS for surveying mountainous land},
