	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Temperament, Depression, Anxiety, Psychopathology},
	Number = {1},
	Pages = {61 - 69},
	Title = {The relationship between parental diagnosis, offspring temperament and offspring psychopathology: a longitudinal analysis},
