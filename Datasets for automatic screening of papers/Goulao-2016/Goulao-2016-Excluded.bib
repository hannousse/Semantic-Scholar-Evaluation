	Journal = {SIGMETRICS Perform. Eval. Rev.},
	Keywords = {ESB SOA Performance and Scalability; Real-world Performance Modelling; Modleling approaches for Enterprises},
	Number = {3},
	Numpages = {1},
	Pages = {12--12},
	Publisher = {ACM},
	Title = {Real-world Performance Modelling of Enterprise Service Oriented Architectures: Delivering Business Value with Complexity and Constraints (Abstracts Only)},
