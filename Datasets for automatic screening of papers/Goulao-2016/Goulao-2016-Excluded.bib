	Keywords = {Depressive disorders, Functioning, Measurement, Occupation, Social, Quality of life},
	Note = {Improving the Clinical Effectiveness of Antidepressant Treatment},
	Pages = {9-13},
	Title = {Clinical effectiveness: The importance of psychosocial functioning outcomes},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165032711001455},
