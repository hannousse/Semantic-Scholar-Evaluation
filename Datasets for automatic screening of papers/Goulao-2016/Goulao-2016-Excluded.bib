@article{HOWELL2001165,
	Abstract = {Generalized anxiety disorder (GAD) was introduced into the psychiatric nomenclature in 1980 as an attempt to differentiate chronic excessive worry from paroxysmal attacks of anxiety consistent with panic disorder.3 Since that time, the diagnostic category has undergone substantial refinement. Initially, a diagnosis of GAD was eclipsed in the setting of selected other psychiatric diagnoses, including major depressive disorder (MDD) and psychotic disorders.3, 4 With the publication of the DSM-III-R, the minimum duration of illness was extended from 1 month to 6 months. In the DSM-IV, the duration of illness was retained, but the exclusionary rule in relation to MDD or psychotic disorders was eliminated. Thus, the variations in clinical criteria since GAD was introduced may have influenced what can be said definitively about its epidemiology, course, and clinical correlates. GAD, according to the DSM-IV, is excessive anxiety and worry occurring more days than not for a period of at least 6 months.5 Also, accompanying the primary anxiety and worry are restlessness, fatigue, difficulty concentrating, irritability, muscle tension, disturbed sleep or a combination of these. The worry is pervasive and intrusive to daily functioning. These symptoms are experienced in far greater proportion than the current life situation would warrant. Three centers from the Epidemiological Catchment Area (ECA) study queried participants about generalized anxiety using DSM-III criteria. The rates are shown in Table 1. As can be seen, the 12-month prevalence rate in women is approximately double the rate in men whether the comorbidity exclusions are suspended (2.4% in men and 5.0% in women) or left in place (1.9% in men and 3.4% in women).8 There are substantial variations in the likelihood of suffering from GAD for various age and ethnic groups (Table 2). The highest 12-month rates for GAD (with suspension of exclusionary criteria) are found in African-American women less than age 30 years and Hispanic women aged 45 to 64 years. In both instances, the rate was more than 10%.8 The preponderance of women with GAD is confirmed by data from the National Comorbidity Survey (NCS).35 The 12-month prevalence rate for GAD using DSM-III-R criteria (which commanded a 6-month duration of illness) was 2.0% in men and 4.3% in women. The lifetime prevalence rates were 3.6% in men and 6.6% in women. The similarity in 12-month prevalence rates in ECA and NCS are notable, especially given the different definitions in illness duration. This reflects the fact that, although DSM-III stipulated only a 1-month duration of symptoms, 40% of patients were symptomatic for 5 years or longer.8 GAD commonly is found among patients seen in primary care settings. The Collaborative Study on Psychological Problems in General Health Care18 investigated the rate of several common psychiatric illnesses across 15 primary care centers on four continents. The investigators suggest that varying prevalence rates among countries support a psychosocial cause to the sex differences for the disorder, while similar prevalence rates worldwide may support biologically based sex differences in the cause of illness. The investigators calculated the prevalence and gender ratios for MDD, panic disorder with or without agoraphobia, and GAD. Although rates for MDD and panic disorder among men and women presenting to a primary care setting across countries were not significantly different, there were significant gender by center interactions, indicating a lack of consistency for GAD. The average point prevalence of GAD in men was 5.7%, whereas in women it was 9.2% (N = 705). The highest percentage (25.8%) of anxious women was found in Brazil (compared with 14.1% of Brazilian men) and the lowest rate of anxious women was found in Turkey (1.1%, compared with 0.5% of anxious Turkish men). Interestingly, the second lowest percentage rating of anxious women (1.4%) was found in Seattle, Washington, the only US site, and one of the three sites where men reported higher frequency of GAD than did women (3.7% vs 1.4%). Given the inconsistency in GAD among the various centers, the investigators suggest that this anxiety disorder is connected to gender-specific social roles or experiences.},
	Author = {Heather B. Howell and Olga Brawman-Mintzer and Jeannine Monnier and Kimberly A. Yonkers},
	Date-Added = {2019-09-01 17:23:45 +0100},
	Date-Modified = {2019-09-01 17:23:45 +0100},
	Doi = {https://doi.org/10.1016/S0193-953X(05)70212-4},
	Issn = {0193-953X},
	Journal = {Psychiatric Clinics of North America},
	Number = {1},
	Pages = {165 - 178},
	Title = {Generalized Anxiety Disorder in Women},
	Url = {http://www.sciencedirect.com/science/article/pii/S0193953X05702124},
	Volume = {24},
	Year = {2001},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0193953X05702124},
	Bdsk-Url-2 = {https://doi.org/10.1016/S0193-953X(05)70212-4}}

@article{BEINE2001713,
	Abstract = {Since Auerbach and Kotlikoff's [(1987). Dynamic fiscal policy. Cambridge: Cambridge University Press] studies, the overlapping generations (OLG) model has become a useful modelling tool of policy evaluation. Given the lack of data on life-cycle behaviour, the parameterisation of such models is cumbersome. The purpose of this paper is to offer an econometrics-based method for estimating the utility function parameters. These parameters determine both the short-run responses to policy shocks and the speed of convergence toward the long-run solution. We use the US Consumer Expenditure Survey data and create a pseudopanel data bank with representative agents. Our estimators are obtained by compiling generalised method of moments (GMM) yearly values following the minimum distance estimation (MDE) of Chamberlain [(1984). Panel data. In Z. Griliches & M. D. Intriligator (Eds.), Handbook of econometrics II (pp. 1247--1318). Amsterdam: North-Holland]. It is shown that leisure is not an explicit argument of the utility function, the intertemporal elasticity of substitution is rather low, and aggregated purchases of goods present some durability.},
	Author = {Michel Beine and Francis Bismans and Fr{\'e}deric Docquier and S{\'e}bastien Laurent},
	Date-Added = {2019-09-01 17:23:45 +0100},
	Date-Modified = {2019-09-01 17:23:45 +0100},
	Doi = {https://doi.org/10.1016/S0161-8938(01)00087-4},
	Issn = {0161-8938},
	Journal = {Journal of Policy Modeling},
	Keywords = {OLG model, GMM estimation, Pseudopanel},
	Number = {7},
	Pages = {713 - 729},
	Title = {Life-cycle behaviour of US households: A nonlinear GMM estimation on pseudopanel data},
	Url = {http://www.sciencedirect.com/science/article/pii/S0161893801000874},
	Volume = {23},
	Year = {2001},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0161893801000874},
