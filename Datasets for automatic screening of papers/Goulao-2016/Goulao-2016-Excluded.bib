	Issn = {0376-8716},
	Journal = {Drug and Alcohol Dependence},
	Keywords = {Alcohol use, Abstention, MDE, Anxiety disorder, Suicidal thoughts},
	Pages = {245 - 251},
	Title = {Associations of lifetime abstention and past and current alcohol use with late-life mental health: A propensity score analysis},
