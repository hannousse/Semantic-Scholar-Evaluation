	Keywords = {Kenya, Ghana, Norway, The UK, Depression, Sociodemographic factors, Status characteristics},
	Pages = {154 - 160},
	Title = {Cross-country variation in the sociodemographic factors associated with major depressive episode in Norway, the United Kingdom, Ghana, and Kenya},
