	Journal = {Journal of Affective Disorders},
	Keywords = {Depression, Depressive disorders, Subthreshold, Minor, Risk factors, Outcome},
	Number = {1},
	Pages = {7 - 22},
	Title = {Subthreshold depression in children and adolescents -- a systematic review},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165032713004874},
