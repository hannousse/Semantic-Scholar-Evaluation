	Journal = {Journal of Affective Disorders},
	Keywords = {Major depressive disorder, Selegiline transdermal system, Tyramine, Antidepressants, Monoamine oxidase inhibitors, Safety},
	Number = {1},
	Pages = {15 - 23},
	Title = {The selegiline transdermal system in major depressive disorder: A systematic review of safety and tolerability},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165032707001486},
