	Journal = {Psychiatry Research},
	Keywords = {Major depression, Substance abuse, Substance dependence, Alcohol, Drug, Comorbidity, Order of onset},
	Number = {2},
	Pages = {248 - 253},
	Title = {Associations between subtypes of major depressive episodes and substance use disorders},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165178110006281},
	Volume = {186},
	Year = {2011},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0165178110006281},
