	Journal = {Journal of Affective Disorders},
	Keywords = {Major depressive disorder (MDD), Remission, Treatment outcome},
	Number = {1},
	Pages = {79 - 84},
	Title = {The impact of gender, age and depressed state on patients' perspectives of remission},
