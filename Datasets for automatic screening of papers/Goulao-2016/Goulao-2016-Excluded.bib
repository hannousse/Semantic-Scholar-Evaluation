	Journal = {Drug and Alcohol Dependence},
	Keywords = {Nonmedical prescription use, Major depression, Age of onset, Etiology},
	Number = {1},
	Pages = {99 - 104},
	Title = {Exploring age of onset as a causal link between major depression and nonmedical use of prescription medications},
