	Journal = {Neuroscience & Biobehavioral Reviews},
	Keywords = {Magnetic resonance spectroscopy (MRS), Meta-analysis, Anterior cingulate cortex, Major depressive disorder, Glutamate, Glutamine},
	Number = {1},
	Pages = {198 - 205},
	Title = {Region and state specific glutamate downregulation in major depressive disorder: A meta-analysis of 1H-MRS findings},
	Url = {http://www.sciencedirect.com/science/article/pii/S0149763411001096},
	Volume = {36},
	Year = {2012},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0149763411001096},
