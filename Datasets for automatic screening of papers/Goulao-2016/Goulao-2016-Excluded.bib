	Journal = {Psychoneuroendocrinology},
	Keywords = {Depression, Major depressive disorder, Bipolar disorder, Oxidative stress, 8-Hydroxy-2′-deoxyguanosine (8-OHdG), F2-isoprostanes},
	Note = {This issue includes a Special Section on Biomarkers in the Military - New Findings from Prospective Studies},
	Pages = {164 - 175},
	Title = {Is depression associated with increased oxidative stress? A systematic review and meta-analysis},
