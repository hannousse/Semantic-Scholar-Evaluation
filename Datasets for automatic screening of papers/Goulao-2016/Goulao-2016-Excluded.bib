	Keywords = {Mood Disorder Questionnaire, Bipolar disorders, Screening, Quality of life, Spectrum},
	Pages = {173 - 178},
	Title = {Does Mood Disorder Questionnaire identify sub-threshold bipolarity? Evidence studying worsening of quality of life},
