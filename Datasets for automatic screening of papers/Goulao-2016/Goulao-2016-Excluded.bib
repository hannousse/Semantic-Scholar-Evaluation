	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Resting state, fMRI, Connectivity, Depression, Treatment, Default-mode},
	Pages = {8 - 17},
	Title = {A systematic review of relations between resting-state functional-MRI and treatment response in major depressive disorder},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165032714005825},
