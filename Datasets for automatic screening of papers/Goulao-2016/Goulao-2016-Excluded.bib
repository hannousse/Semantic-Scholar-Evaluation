	Issn = {0890-8567},
	Journal = {Journal of the American Academy of Child & Adolescent Psychiatry},
	Keywords = {adolescents, co-occurring disorders, depression treatment, health insurance, substance use treatment},
	Number = {9},
	Pages = {953 - 960},
	Title = {Health Insurance and Treatment of Adolescents With Co-Occurring Major Depression and Substance Use Disorders},
