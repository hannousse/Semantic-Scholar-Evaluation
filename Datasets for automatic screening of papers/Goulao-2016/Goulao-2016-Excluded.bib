	Doi = {https://doi.org/10.1016/j.jad.2013.09.028},
	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Questionnaire, Bipolar disorder, Detection, Hypomania, DSM-IV-TR},
	Pages = {295 - 298},
	Title = {DSM-IV diagnosis in depressed primary care patients with previous psychiatric ICD-10 bipolar disorder},
