	Journal = {Journal of Affective Disorders},
	Keywords = {Pain, Bipolar disorder, Comorbidity, Epidemiologic},
	Number = {3},
	Pages = {151 - 156},
	Title = {Factors associated with pain interference in an epidemiologic sample of adults with bipolar I disorder},
	Url = {http://www.sciencedirect.com/science/article/pii/S0165032709000317},
	Volume = {117},
	Year = {2009},
	Bdsk-Url-1 = {http://www.sciencedirect.com/science/article/pii/S0165032709000317},
