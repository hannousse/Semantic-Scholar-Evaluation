	Journal = {Journal of Affective Disorders},
	Keywords = {Acupuncture, Major depressive disorder, Post-stroke depression, Meta-analysis},
	Number = {1},
	Pages = {9 - 21},
	Title = {The effectiveness and safety of acupuncture therapy in depressive disorders: Systematic review and meta-analysis},
