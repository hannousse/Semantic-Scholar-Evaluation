	Doi = {https://doi.org/10.1016/j.jad.2015.08.033},
	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Deep transcranial magnetic stimulation (DTMS), Major depressive disorder (MDD), Systematic literature review, Meta-analysis},
	Pages = {73 - 83},
	Title = {Deep transcranial magnetic stimulation (DTMS) in the treatment of major depression: An exploratory systematic review and meta-analysis},
