	Issn = {0306-4530},
	Journal = {Psychoneuroendocrinology},
	Keywords = {Depression, Biomarker, Vascular endothelial growth factor, Meta-analysis, Major depressive disorder, Neuronal plasticity},
	Pages = {18 - 26},
	Title = {Peripheral vascular endothelial growth factor as a novel depression biomarker: A meta-analysis},
