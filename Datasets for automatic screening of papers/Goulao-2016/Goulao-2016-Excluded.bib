	Doi = {https://doi.org/10.1016/j.nbd.2012.05.007},
	Issn = {0969-9961},
	Journal = {Neurobiology of Disease},
	Keywords = {Intermediate phenotype, Depression, Fmri, Gene, Dopamine, Reward, Stress, CRH, HPA axis},
	Pages = {12 - 23},
	Title = {Neurogenetics of depression: A focus on reward processing and stress sensitivity},
