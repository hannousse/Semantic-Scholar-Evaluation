	Issn = {0950-5849},
	Journal = {Information and Software Technology},
	Keywords = {Safety certification, Safety standards, Safety evidence, Model-driven engineering, UML, UML profiles},
	Number = {5},
	Pages = {836 - 864},
	Title = {Supporting the verification of compliance to safety standards via model-driven engineering: Approach, tool-support and empirical validation},
	Url = {http://www.sciencedirect.com/science/article/pii/S0950584912002352},
	Volume = {55},
