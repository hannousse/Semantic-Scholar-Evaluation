	Issn = {1543-5946},
	Journal = {The American Journal of Geriatric Pharmacotherapy},
	Keywords = {depression, anxiety, geriatric, escitalopram, elderly},
	Number = {3},
	Pages = {201 - 209},
	Title = {Escitalopram for comorbid depression and anxiety in elderly patients: A 12-week, open-label, flexible-dose, pilot trial},
