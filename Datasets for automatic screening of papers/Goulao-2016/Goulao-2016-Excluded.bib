	Isbn = {978-1-4503-2467-0},
	Keywords = {aadl, error analysis, error modeling, formal architecture, hazard analysis, risk assessment},
	Location = {Pittsburgh, Pennsylvania, USA},
	Numpages = {20},
	Pages = {65--84},
	Publisher = {ACM},
	Series = {HILT '13},
	Title = {Illustrating the AADL Error Modeling Annex (V.2) Using a Simple Safety-critical Medical Device},
	Url = {http://doi.acm.org/10.1145/2527269.2527271},
	Year = {2013},
