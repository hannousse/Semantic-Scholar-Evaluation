	Issn = {0272-7358},
	Journal = {Clinical Psychology Review},
	Keywords = {PTSD, Trauma, Comorbidity, Depression, Cortisol},
	Number = {4},
	Pages = {301 - 315},
	Title = {Relations among posttraumatic stress disorder, comorbid major depression, and HPA function: A systematic review and meta-analysis},
	Url = {http://www.sciencedirect.com/science/article/pii/S0272735812000244},
	Volume = {32},
