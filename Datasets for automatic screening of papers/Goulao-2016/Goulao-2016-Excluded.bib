	Issn = {0165-0327},
	Journal = {Journal of Affective Disorders},
	Keywords = {Bipolar disorder, major depressive disorder, general population, neuroticism, extraversion, personality},
	Number = {1},
	Pages = {42 - 52},
	Title = {Differences in neuroticism and extraversion between patients with bipolar I or II and general population subjects or major depressive disorder patients},
