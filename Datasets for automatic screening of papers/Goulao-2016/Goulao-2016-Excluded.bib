	Issn = {0278-5846},
	Journal = {Progress in Neuro-Psychopharmacology and Biological Psychiatry},
	Keywords = {Anticonvulsant, Major depression, Topiramate},
	Note = {Suicide and suicidal behavior},
	Number = {4},
	Pages = {970 - 973},
	Title = {Topiramate augmentation in patients with resistant major depressive disorder: A double-blind placebo-controlled clinical trial},
