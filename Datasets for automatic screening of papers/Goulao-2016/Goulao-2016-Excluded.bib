	Journal = {Progress in Biophysics and Molecular Biology},
	Keywords = {Bioinformatics, Systems biology, Genome, Protein, RIKEN, Japan},
	Note = {Focussed issue: Annotating the Genome},
	Number = {1},
	Pages = {23 - 42},
	Title = {Bioinformatics for the genomic sciences and towards systems biology. Japanese activities in the post-genome era},
