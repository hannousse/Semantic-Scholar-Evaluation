	Journal = {Drug and Alcohol Dependence},
	Keywords = {Smoking, Depression, Recurrent depression, Nicotine dependence, Item response theory},
	Number = {1},
	Pages = {90 - 95},
	Title = {Single versus recurrent depression history: Differentiating risk factors among current US smokers},
